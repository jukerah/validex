{
  "name": "vkrun",
  "version": "0.36.0",
  "description": "Vkrun is a Node.js framework for building server-side applications",
  "author": "Mario Elvio",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "vkrun",
    "vkrunjs",
    "framework",
    "web",
    "http",
    "rest",
    "restful",
    "router",
    "app",
    "api",
    "schema",
    "cors",
    "validate",
    "validator",
    "rate limit",
    "parse data",
    "jwt",
    "logger",
    "session",
    "super request",
    "backend",
    "back-end"
  ],
  "files": [
    "LICENSE",
    "Readme.md",
    "lib"
  ],
  "homepage": "https://github.com/vkrunjs/vkrunjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkrunjs/vkrunjs.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage=false",
    "test:e2e": "jest --coverage --config=jest-e2e.config.ts",
    "test:all": "jest --coverage=false --config=jest-all.config.ts",
    "test:all-c": "jest --coverage --config=jest-all.config.ts",
    "test:coverage": "jest",
    "test:staged": "jest --passWithNoTests",
    "test:ci": "npm test -- --coverage=false",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --ext *.ts --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.6.7",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "*"
  }
}
